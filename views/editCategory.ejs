<%- include("partials/header") %>

    <div class="container">
        <div class="form-group text-center">
            <form action="/category/<%=foundData._id%>?_method=put" method="post">
                <label class="d-block text-center my-3" for="categoryName">Update Category Name</label>
                <input type="text" class="form-control" name="categoryName" value="<%=foundData.name%>"
                    placeholder="Update category">
                <button class="btn text-center my-3 rounded-pill btn-primary "> Confirm</button>
            </form>
        </div>
    </div>
    <%- include("partials/footer") %>